{
  "name": "fw-devtools",
  "description": "开发辅助工具",
  "version": "2.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "devtoos for FW Company",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://fe.9888.cn/*",
        "https://fe.9888.cn/*",
        "https://m.easyloan888.com/*",
        "https://m.9888.cn/*"
      ],
      "js": [
        "fix-viewport.js"
      ],
      "run_at": "document_end"
    }
  ],
  "homepage_url": "https://github.com/delongw/chrome-extensions",
  "offline_enabled": true,
  "short_name": "fw",
  "manifest_version": 2
}